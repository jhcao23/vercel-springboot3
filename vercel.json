{
  "version": 2,
  "builds": [
    {
      "src": "download.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["your-spring-boot-app"]
      }
    },
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "server.js" }
  ]
}
